@{
    ViewData["Title"] = "Alumnos > Detalle";
}
@model Domain.Alumno
<dl class="row">
    <dt class="col-sm-2">Id</dt>
    <dd class="col-sm-10">@Model.Id</dd>
    <dt class="col-sm-2">Nombre</dt>
    <dd class="col-sm-10">@Model.Nombre</dd>
    <dt class="col-sm-2">Edad</dt>
    <dd class="col-sm-10">@Model.Edad</dd>
    <dt class="col-sm-2">Promedio</dt>
    <dd class="col-sm-10">@Model.Promedio</dd>
    <dt class="col-sm-2">Foto</dt>
    <dd class="col-sm-10"><img src="@Model.Foto" class="img-thumbnail" /></dd>
</dl>
<a asp-action="Index" class="btn btn-danger mt-3">Regresar</a>
